{
  "name": "nnls",
  "version": "3.0.0",
  "description": "Non-Negative Least-Squares",
  "main": "./lib/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib/index.d.ts",
  "keywords": [
    "nnls",
    "non-negative",
    "least-squares",
    "optimization",
    "matrix",
    "linear-algebra",
    "ml"
  ],
  "author": "Santi Miranda",
  "license": "MIT",
  "files": [
    "src",
    "lib",
    "lib-esm"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "eslint": "eslint src --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run tsc",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "vitest run --coverage",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mljs/nnls.git"
  },
  "bugs": {
    "url": "https://github.com/mljs/nnls/issues"
  },
  "homepage": "https://github.com/mljs/nnls#readme",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.50.0",
    "eslint-config-cheminfo-typescript": "^12.0.4",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "ml-matrix": "^6.10.5"
  }
}